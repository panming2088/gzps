public class DayPatrolFilterConditionEvent {

    /**
     * 筛选的区域
     */
    private String distrct = null;
    /**
     * 设施类型：窨井，雨水口等
     */
    private String facilityType = null;
    private Long startTime;

    private Long endTime;

    public DayPatrolFilterConditionEvent(String distrct, String facilityType,
                                              Long startTime,Long endTime) {
        this.distrct = distrct;
        this.facilityType = facilityType;
        this.startTime = startTime;
        this.endTime = endTime;
    }

    public String getFacilityType() {
        return facilityType;
    }

    public String getQueryDistrict() {
        return distrct;
    }

    public Long getStartTime() {
        return startTime;
    }

    public Long getEndTime() {
        return endTime;
    }
}
